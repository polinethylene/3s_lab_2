QComboBox QAbstractItemView {
    border: 2px solid #f46b60;
    background-color: #f46b60;
}

QLineEdit {
    color: #f46b60;
    font: bold 12px;
    background-color: #FBF4E8;
    border-style: outset;
    border-width: 3px;
    border-color: #90c1bc;
    padding: 6px;
    selection-background-color: #90c1bc;
}

QTextEdit {
    color: #90c1bc;
    font: 12px;
    background-color: #FBF4E8;
    border-style: outset;
    border-width: 3px;
    border-radius: 10px;
    border-color: #90c1bc;
    padding: 6px;
    selection-background-color: #90c1bc;
}

